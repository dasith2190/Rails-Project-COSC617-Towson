<p id="notice"><%= notice %></p>

<h1>Listing Postcs</h1>


<table>
  <thead>
    <tr>
      <th>User idfk</th>
      <th></th>
      <th>Post title</th>
      <th></th>
      <th>Post description</th>
      <th></th>
      <th>Address</th>
      <th></th>
      <th>Country</th>
      <th></th>
      <th>Zip</th>
      <th></th>
      <th>City</th>
      <th></th>
      <th>Price</th>
      <th>Home type</th>
      <th></th>
      <th>Rooms</th>
      <th></th>
      <th>Baths</th>
      <th></th>
      <th>Price</th>
      <th></th>
      <th>Price type</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postcs.each do |postc| %>
      <tr>
        <td><%= postc.user_idfk %></td>
        <td><%= postc.post_title %></td>
        <td><%= postc.post_description %></td>
        <td><%= postc.address %></td>
        <td><%= postc.country %></td>
        <td><%= postc.zip %></td>
        <td><%= postc.city %></td>
        <td><%= postc.price %></td>
        <td><%= postc.home_type %></td>
        <td><%= postc.rooms %></td>
        <td><%= postc.baths %></td>
        <td><%= postc.price %></td>
        <td><%= postc.price_type %></td>
        <td><%= link_to 'Show', postc %></td>
        <td><%= link_to 'Edit', edit_postc_path(postc) %></td>
        <td><%= link_to 'Destroy', postc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

